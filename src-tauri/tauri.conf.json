	"identifier": "com.cmd-golem.group-renaming",
	"build": {
		"frontendDist": "../src"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"label": "main",
				"title": "Group Renaming",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false,
				"dragDropEnabled": false,
