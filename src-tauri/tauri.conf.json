		"security": {
			"csp": null,
			"assetProtocol": {
				"enable": true,
				"scope": []
			}
		}
	},
	"bundle": {
		"active": true,
		"createUpdaterArtifacts": true,
		"targets": "msi",
		"icon": [
			"icons/icon.png",
